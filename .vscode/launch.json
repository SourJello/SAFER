{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "1",
            "pipeTransport": {
                "pipeProgram": "docker",
                "pipeArgs": [
                    "exec",
                    "-i",
                    "safer-safer-api-1",
                    "sh",
                    "-c"
                ],
                "debuggerPath": "/vsdbg/vsdbg",
                "pipeCwd": "${workspaceFolder}"
            },
            "sourceFileMap": {
                "/workspace/SAFER.API": "${workspaceFolder}/SAFER.API"
            }
        }
    ]
  }
  